   ],
   "source": [
    "num=int(input(\"Enter a number\"))\n",
    "fact=1\n",
    "if num<0:\n",
    "    print(\"Factorial does not exist for negative number\")\n",
    "elif num==0:\n",
    "    print(\"Factorial of 0 is 1\")\n",
    "else:\n",
    "    for i in range(1,num+1):\n",
    "        fact=fact*i\n",
    "        print(f\"The factorial of {num} is {fact}\")"
   ]
  }
