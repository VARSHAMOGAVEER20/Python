    "\n",
    "#Single Inheritance\n",
    "class Parent:\n",
    "    def display(self):\n",
    "        print(\"This is parent class\")\n",
    "        class child(Parent):\n",
    "            def show(self):\n",
    "                print(\"This is child class\")\n",
    "        obj=child() \n",
    "        obj.display()\n",
    "        obj.show() "
