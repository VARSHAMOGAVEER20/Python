    "def is_palindrome(s):\n",
    "    s=str(s)\n",
    "    if s==s[1]:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    word=input(\"Enter a number or word\")\n",
    "    if is_palindrome(word):\n",
    "        print(\"Its a palindrome\")\n",
    "    else:\n",
    "        print(\"Its not a palindrome\")"
   ]
